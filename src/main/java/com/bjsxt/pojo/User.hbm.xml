<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping package="com.bjsxt.pojo">
       <class name="User" table="u_table">
       <id name="uid" column="uid" type="java.lang.Integer">
       <generator class="native"></generator>     
       </id>
       <property name="uname" column="uname" type="java.lang.String"></property>
       <property name="uage" column="uage" type="java.lang.Integer"></property>
       
       </class>   
    
    </hibernate-mapping>